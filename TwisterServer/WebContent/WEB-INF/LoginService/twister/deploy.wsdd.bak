<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from LoginService WSDL service -->

  <service name="Login" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://twister"/>
      <parameter name="wsdlServiceElement" value="LoginService"/>
      <parameter name="schemaQualified" value="http://twister"/>
      <parameter name="wsdlServicePort" value="Login"/>
      <parameter name="className" value="twister.LoginSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Login"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getAllPosts" qname="operNS:getAllPosts" xmlns:operNS="http://twister" returnQName="retNS:getAllPostsReturn" xmlns:retNS="http://twister" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:username" xmlns:pns="http://twister" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="verifyUser" qname="operNS:verifyUser" xmlns:operNS="http://twister" returnQName="retNS:verifyUserReturn" xmlns:retNS="http://twister" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:userInfo" xmlns:pns="http://twister" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="verifyUser getAllPosts"/>

  </service>
</deployment>
